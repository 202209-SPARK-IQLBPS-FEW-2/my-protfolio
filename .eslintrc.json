{
    "settings": {
      "react": {
        "version": "detect"
      }
    },
  
    "env": {
    "browser": true,
    "node": true,
    "es2020": true,
    "jest": true
    },

    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
  }
    },
    "plugins": [
        "react"
    ],

    "rules": {
        "react/prop-types": "off"
      }
  
}
