{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest", 
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": "off",
        "linebreak-style": 0
    },

        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
        
    }
